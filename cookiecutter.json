{
    "project_name": "NeneCppProject",
    "project_slug": "{{ cookiecutter.project_name | lower | replace(' ', '_') | replace('-', '_') }}",

    "cpp_standard": ["23", "20", "17"],
    "default_build_type": ["Debug", "Release", "RelWithDebInfo", "MinSizeRel"],

    "use_clang_tidy": ["y", "n"],
    "use_sanitizers": ["y", "n"],
    "warnings_as_errors": ["y", "n"],

    "author_name": "Your Name",
    "author_email": "your_email@dress.com",

    "use_tests": ["y", "n"],
    "test_framework": ["none", "Catch2", "doctest"],
    "use_subtree": ["y", "n"],
    "use_assets": ["y", "n"],
    "use_documents": ["y", "n"],
    "add_readme_for_users": ["y", "n"],
    "license": ["MIT", "Apache-2.0", "BSD-3-Clause", "Proprietary", "none"],
    "use_package_init": ["y", "n"],

    "init_git": ["y", "n"],
    "git_config_local": ["y", "n"],

    "_copy_without_render": [
    "subtree/**"
  ]
}